from pathlib import Path
import sys

# Dapatkan jalur direktori instalasi Python
python_installation_path = Path(sys.prefix)

# Buat jalur ke direktori Scripts
scripts_path = python_installation_path / 'Scripts'

print("Python installation path:", python_installation_path)
